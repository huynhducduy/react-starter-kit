{
  "name": "react-starter-kit",
  "version": "0.1.0",
  "license": "SEE LICENSE IN LICENSE",
  "private": false,
  "sideEffects": false,
  "scripts": {
    "start": "duy-react-scripts start",
    "start:server": "node server/index.js",
    "build": "duy-react-scripts build",
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "test": "duy-react-scripts test --detectOpenHandles",
    "test:debug": "duy-react-scripts --inspect-brk test --runInBand --no-cache --detectOpenHandles",
    "test:coverage": "run-p 'test --coverage --coverageReporters=cobertura --watchAll=false'",
    "refresh": "rm -rf node_modules && rm -rf yarn.lock && yarn cache clean && yarn",
    "check-types": "tsc",
    "check-js": "eslint 'src/**/*.{js,jsx,mjs,ts,tsx,json}'",
    "lint:code": "run-p check-js check-types",
    "lint:style": "prettier 'src/**/*.{scss,sass,css,pcss}'",
    "lint": "run-p lint:*",
    "fix:code": " eslint --fix 'src/**/*.{js,jsx,mjs,ts,tsx,json}'",
    "fix:style": "prettier --write 'src/**/*.{scss,sass,css,pcss}'",
    "fix": "run-p fix:*",
    "postinstall": "husky install",
    "cypress": "cypress open",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run"
  },
  "optionalDependencies": {
    "fastify": "^3.14.0"
  },
  "dependencies": {
    "@tailwindcss/typography": "^0.4.0",
    "axios": "^0.21.1",
    "classnames": "^2.2.6",
    "date-fns": "^2.17.0",
    "flat": "^5.0.2",
    "framer-motion": "^3.10.0",
    "hoist-non-react-statics": "^3.3.2",
    "husky": "^5.1.1",
    "i18next": "^19.9.2",
    "i18next-browser-languagedetector": "^6.0.1",
    "i18next-http-backend": "^1.1.1",
    "immer": "^8.0.1",
    "npm-run-all": "^4.1.5",
    "path-to-regexp": "^6.2.0",
    "ramda": "^0.27.1",
    "react": "^17.0.1",
    "react-component-composer": "^1.0.0",
    "react-content-loader": "^6.0.2",
    "react-dom": "^17.0.1",
    "react-helmet-async": "^1.0.7",
    "react-i18next": "^11.8.10",
    "react-router-dom": "^5.2.0",
    "react-toastify": "^7.0.3",
    "recoil": "^0.1.2",
    "tailwindcss": "^2.0.3",
    "typescript": "^4.2.2",
    "web-vitals": "^1.1.0"
  },
  "devDependencies": {
    "@testing-library/dom": "^7.30.3",
    "@testing-library/jest-dom": "^5.11.10",
    "@testing-library/react": "^11.2.6",
    "@testing-library/react-hooks": "^5.1.1",
    "@testing-library/user-event": "^13.1.5",
    "@types/flat": "^5.0.1",
    "@types/hoist-non-react-statics": "^3.3.1",
    "@types/jest": "^26.0.22",
    "@types/node": "^14.14.41",
    "@types/ramda": "^0.27.40",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.3",
    "@types/react-router-dom": "^5.1.7",
    "@types/webpack-env": "^1.16.0",
    "autoprefixer": "^10.2.5",
    "babel-preset-duy": "^1.0.4",
    "browserslist": "^4.16.4",
    "cypress": "^6.9.1",
    "duy-react-scripts": "^1.0.13",
    "eslint": "^7.24.0",
    "eslint-config-duy": "^1.0.7",
    "eslint-plugin-cypress": "^2.11.2",
    "eslint-plugin-jest": "^24.3.5",
    "eslint-plugin-jest-dom": "^3.8.0",
    "eslint-plugin-testing-library": "^4.1.0",
    "lint-staged": "^10.5.4",
    "postcss": "^8.2.10",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-normalize": "^9.0.0",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^2.2.1",
    "source-map-explorer": "^2.5.2"
  }
}
